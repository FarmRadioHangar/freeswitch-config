{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "freeswitch",
      "image": "dwilkie/freeswitch-rayo",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5222,
          "containerPort": 5222,
          "protocol": "tcp"
        },
        {
          "hostPort": 8021,
          "containerPort": 8021,
          "protocol": "tcp"
        },
        {
          "hostPort": 5060,
          "containerPort": 5060,
          "protocol": "udp"
        }
      ],
      "environment": [
        {
          "name": "SECRETS_BUCKET_NAME",
          "value": "somlengsecrets"
        },
        {
          "name": "SECRETS_FILE_NAME",
          "value": "freeswitch_secrets.xml"
        },
      ]
    }
  ]
}
