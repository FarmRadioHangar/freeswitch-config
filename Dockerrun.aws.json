{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "freeswitch",
      "image": "somleng/somleng-freeswitch",
      "essential": true,
      "memory": 750,
      "portMappings": [
        {
          "hostPort": 5222,
          "containerPort": 5222,
          "protocol": "tcp"
        },
        {
          "hostPort": 5060,
          "containerPort": 5060,
          "protocol": "udp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/elasticbeanstalk/somleng-freeswitch-webserver/var/log/containers/freeswitch.log",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "awslogs-freeswitch"
        }
      }
    }
  ]
}
